<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Nucleos\AllInklBundle\Command\SpaceCheckCommand">
      <tag name="console.command" command="allinkl:check"/>
      <argument type="service" id="nucleos_allinkl.service.auth"/>
      <argument type="service" id="nucleos_allinkl.service.statistic"/>
      <argument type="service" id="Nucleos\AllInklBundle\Mailer\Mailer"/>
      <argument>%nucleos_allinkl.check.accounts%</argument>
      <argument>%nucleos_allinkl.check.warning%</argument>
    </service>
  </services>
</container>
